@ARTICLE{Gu2023-gr,
  title         = "{A Systematic Survey of Prompt Engineering on Vision-Language
                   Foundation Models}",
  author        = "Gu, Jindong and Han, Zhen and Chen, Shuo and Beirami, Ahmad
